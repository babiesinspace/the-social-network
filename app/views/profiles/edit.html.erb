<%= form_for profile_path(@user) do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>
  <br/>
  <%= f.label :birthday %>
  <%= f.date_field :birthday %>
  <br/>
  <%= f.label :hometown %>
  <%= f.text_field :hometown %>
  <br/>
  <%= f.label :current_town %>
  <%= f.text_field :current_town %>
  <br/>
  <%= f.label :college %>
  <%= f.text_field :education %>
  <br/>
  <%= f.label :relationship_status %>
  <%= f.text_field :relationship_status %>
  <br/>
  <%= f.submit "Save" %>
<% end %>
